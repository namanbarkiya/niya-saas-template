{
  "useTabs": false,
  "printWidth": 80,
  "tabWidth": 2,
  "singleQuote": false,
  "trailingComma": "es5",
  "semi": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "^next(.*)$",
    "^@supabase/(.*)$",
    "^@tanstack/(.*)$",
    "^@radix-ui/(.*)$",
    "^@hookform/(.*)$",
    "^framer-motion$",
    "^lucide-react$",
    "^sonner$",
    "^zod$",
    "^zustand$",
    "^jotai$",
    "^clsx$",
    "^class-variance-authority$",
    "^tailwind-merge$",
    "^date-fns$",
    "^react-day-picker$",
    "^react-tweet$",
    "^motion$",
    "^tw-animate-css$",
    "^@/(.*)$",
    "^[./]",
    "^.*\\.css$"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"]
}
